{
    "openapi": "3.0.0",
    "info": {
        "title": "AditusAPI",
        "version": "1.0"
    },
    "paths": {
        "/api/sites": {
            "get": {
                "tags": [
                    "Sites"
                ],
                "summary": "Get all sites",
                "operationId": "8cd61005183359799066c5818fa507e3",
                "responses": {
                    "200": {
                        "description": "Sites retrieved successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Sites retrieved successfully"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Failed to retrieve sites",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": false
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Failed to retrieve sites: Internal Server Error"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/services/{product_category}/{site?}": {
            "get": {
                "tags": [
                    "Services"
                ],
                "summary": "Get services by product category and optionally by site",
                "operationId": "f8816185d79bddfa179b9a63730322c1",
                "parameters": [
                    {
                        "name": "product_category",
                        "in": "path",
                        "description": "Slug of the product category",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "example": "cultural-events"
                        }
                    },
                    {
                        "name": "site",
                        "in": "path",
                        "description": "Slug of the site (optional)",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "example": "neapolis"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Services retrieved successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Services retrieved successfully"
                                        },
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "id": {
                                                        "type": "integer",
                                                        "example": 1
                                                    },
                                                    "name": {
                                                        "type": "string",
                                                        "example": "Guided Tour"
                                                    },
                                                    "description": {
                                                        "type": "string",
                                                        "example": "A guided tour of the archaeological site"
                                                    },
                                                    "site_id": {
                                                        "type": "integer",
                                                        "example": 1
                                                    },
                                                    "slug": {
                                                        "type": "string",
                                                        "example": "guided-tour"
                                                    },
                                                    "is_siae": {
                                                        "type": "boolean",
                                                        "example": false
                                                    },
                                                    "site": {
                                                        "properties": {
                                                            "id": {
                                                                "type": "integer",
                                                                "example": 1
                                                            },
                                                            "name": {
                                                                "type": "string",
                                                                "example": "Parco Archeologico della Neapolis"
                                                            },
                                                            "address": {
                                                                "type": "string",
                                                                "example": "Via Paradiso, 14, 96100 Siracusa SR, Italia"
                                                            }
                                                        },
                                                        "type": "object"
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Failed to retrieve services",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": false
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Failed to retrieve services: Internal Server Error"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/services/{slug}/{is_siae}/products": {
            "get": {
                "tags": [
                    "Products"
                ],
                "summary": "Get products by service slug and siae status",
                "operationId": "7a15e6d0e3095284be49b6408dafd761",
                "parameters": [
                    {
                        "name": "slug",
                        "in": "path",
                        "description": "Slug of the service",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "example": "guided-tour"
                        }
                    },
                    {
                        "name": "is_siae",
                        "in": "path",
                        "description": "SIAE status (0 or 1)",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": 0
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Data retrieved successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Data retrieved successfully"
                                        },
                                        "data": {
                                            "properties": {
                                                "service": {
                                                    "properties": {
                                                        "id": {
                                                            "type": "integer",
                                                            "example": 1
                                                        },
                                                        "name": {
                                                            "type": "string",
                                                            "example": "Guided Tour"
                                                        },
                                                        "description": {
                                                            "type": "string",
                                                            "example": "A guided tour of the archaeological site"
                                                        },
                                                        "slug": {
                                                            "type": "string",
                                                            "example": "guided-tour"
                                                        }
                                                    },
                                                    "type": "object"
                                                },
                                                "products": {
                                                    "type": "array",
                                                    "items": {
                                                        "properties": {
                                                            "id": {
                                                                "type": "integer",
                                                                "example": 1
                                                            },
                                                            "name": {
                                                                "type": "string",
                                                                "example": "Full Guided Tour"
                                                            },
                                                            "price_sale": {
                                                                "type": "string",
                                                                "example": "25,00"
                                                            },
                                                            "cod_ordine_posto": {
                                                                "type": "string",
                                                                "example": "AB12345"
                                                            },
                                                            "cod_riduzione_siae": {
                                                                "type": "string",
                                                                "example": "RID01"
                                                            },
                                                            "is_date": {
                                                                "type": "boolean",
                                                                "example": true
                                                            },
                                                            "is_hour": {
                                                                "type": "boolean",
                                                                "example": false
                                                            },
                                                            "reductions": {
                                                                "type": "array",
                                                                "items": {
                                                                    "properties": {
                                                                        "id": {
                                                                            "type": "integer",
                                                                            "example": 1
                                                                        },
                                                                        "name": {
                                                                            "type": "string",
                                                                            "example": "Student Discount"
                                                                        }
                                                                    },
                                                                    "type": "object"
                                                                }
                                                            },
                                                            "reduction_fields": {
                                                                "type": "array",
                                                                "items": {
                                                                    "properties": {
                                                                        "id": {
                                                                            "type": "integer",
                                                                            "example": 1
                                                                        },
                                                                        "name": {
                                                                            "type": "string",
                                                                            "example": "Student Card"
                                                                        },
                                                                        "slug": {
                                                                            "type": "string",
                                                                            "example": "student-card"
                                                                        }
                                                                    },
                                                                    "type": "object"
                                                                }
                                                            },
                                                            "shows": {
                                                                "type": "array",
                                                                "items": {
                                                                    "properties": {
                                                                        "date_event": {
                                                                            "type": "string",
                                                                            "example": "2024-10-25"
                                                                        },
                                                                        "eventoId": {
                                                                            "type": "string",
                                                                            "example": "EV123"
                                                                        },
                                                                        "availability": {
                                                                            "type": "integer",
                                                                            "example": 100
                                                                        },
                                                                        "product_id": {
                                                                            "type": "integer",
                                                                            "example": 1
                                                                        }
                                                                    },
                                                                    "type": "object"
                                                                }
                                                            }
                                                        },
                                                        "type": "object"
                                                    }
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Service not found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": false
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Service not found"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Failed to retrieve data",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": false
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Failed to retrieve data: Internal Server Error"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/evento/{eventoId}/ordineposto/{cod_ordine_posto}/disponibilita": {
            "get": {
                "tags": [
                    "Disponibilita"
                ],
                "summary": "Get availability of a seat by event ID and seat order code",
                "operationId": "b9a3e65eb3ceb33dfd2eb25168ba292c",
                "parameters": [
                    {
                        "name": "eventoId",
                        "in": "path",
                        "description": "ID of the event",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "example": "EV123"
                        }
                    },
                    {
                        "name": "cod_ordine_posto",
                        "in": "path",
                        "description": "Seat order code",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "example": "AB12345"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Posti disponibili retrieved successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Posti disponibili retrieved successfully"
                                        },
                                        "data": {
                                            "properties": {
                                                "disponibilita": {
                                                    "type": "integer",
                                                    "example": 100
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Failed to retrieve availability",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": false
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Failed to retrieve availability: Internal Server Error"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
